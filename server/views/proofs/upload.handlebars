<div class="container">
  <div class="container">
    <h3>Zip Upload</h3>
    <form action="/proofs/upload" method="POST" enctype="multipart/form-data">
      <div class="file-field input-field">
        <div class="btn grey">
          <span>File</span>
          <input name="ziptoUpload" type="file">
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text" placeholder="choose zip file">
        </div>
      </div>
      <button type="submit" class="btn">Upload</button>
    </form>
  </div>
</div>